ine":55,"column":17}},{"generated":{"line":62,"column":4},"source":"src/components/SearchProducts/SearchProducts.jsx","original":{"line":54,"column":12}},{"generated":{"line":62,"column":5},"source":"src/components/SearchProducts/SearchProducts.jsx","original":{"line":53,"column":8}},{"generated":{"line":62,"column":6},"source":"src/components/SearchProducts/SearchProducts.jsx","original":{"line":52,"column":4}},{"generated":{"line":63,"column":0},"source":"src/components/SearchProducts/SearchProducts.jsx","original":{"line":65,"column":1}},{"generated":{"line":63,"column":1},"source":"src/components/SearchProducts/SearchProducts.jsx","original":{"line":50,"column":23}},{"generated":{"line":63,"column":2},"source":"src/components/SearchProducts/SearchProducts.jsx","original":{"line":50,"column":0}}],"sources":{"src/components/SearchProducts/SearchProducts.jsx":"import React from \"react\";\n\nimport { Box, Center, Heading, SimpleGrid, Text } from '@chakra-ui/react'\nimport { useSearchProductsQuery } from \"../../api/store.api\";\nimport Product from \"../Product/Product\";\n\n\n\nexport const SearchProducts = React.memo(({search})=> {\n\n    const {\n        data: products,\n        isSuccess,\n        isError,\n        isLoading,\n        error,\n    } = useSearchProductsQuery(search)\n    \n\n    if(isLoading){\n        return (\n            <Heading>Chargement...</Heading>\n        )\n    }\n\n    if(isError){\n        console.log(error)\n        return (\n            <Center>\n                <Heading>Une erreur c'est produite</Heading>\n            </Center>\n        )\n    }\n\n    if(isSuccess){\n        return (\n            <Box>\n                <Center>\n                    <Text fontSize={'2xl'} fontWeight={'semibold'}>{products.length} Résultas</Text>\n                </Center>\n                <Box>\n                    <ProductDisplay products={products} />\n                </Box>\n            </Box>\n        )\n    }\n})\n\n\nconst ProductDisplay = React.memo(({products})=> {\n\n    return (\n        <Box>\n            <SimpleGrid columns={2}  spacingY={5}>\n                {products.map((product)=> {\n                    return (\n                        <Center margin='5px' key={product.id}>\n                            <Product.Thin  key={product.id} product={product} />\n                        </Center>\n                    )\n                })}\n            </SimpleGrid>\n        </Box>\n    )\n})"},"lineCount":null}},"error":null,"hash":"557111fdc60713d60ce367822ccb7231","cacheData":{"env":{}}}