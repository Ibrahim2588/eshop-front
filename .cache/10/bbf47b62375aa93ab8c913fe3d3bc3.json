d":{"line":59,"column":1},"source":"src/components/CartIcon/CardIcon.jsx","original":{"line":10,"column":7}}],"sources":{"src/components/CartIcon/CardIcon.jsx":"import React from \"react\";\n\nimport { Box, Button, HStack, IconButton, Text, useDisclosure } from '@chakra-ui/react'\nimport { BeakerIcon, ShoppingBagIcon } from '@heroicons/react/outline'\nimport { useSelector } from \"react-redux\";\nimport { useNavigate } from \"react-router-dom\";\nimport { OrdersDisplay } from \"../OrdersDisplay/OrdersDisplay\";\n\n\nexport const CartIcon = ()=> {\n\n    const navigate = useNavigate()\n\n    const isAuthenticated = useSelector(state=> state.user.isAuthenticated)\n\n    const orders = useSelector(state=> state.orders)\n\n\n    const { isOpen, onOpen, onClose } = useDisclosure()\n\n\n    if(isAuthenticated){\n        return (\n            <Box>\n                <Button \n                    onClick={onOpen}\n                    size='sm'\n                    variant='outline'\n                    rounded='lg'\n                    paddingX='5px'\n                    _hover={{bgColor: 'gray.800'}}\n                    colorScheme='red'\n                    leftIcon={<ShoppingBagIcon  width='24' height='24' />}\n                >\n                    <Text fontSize='xl' >{orders.length}</Text>\n                </Button>\n\n                <OrdersDisplay isOpen={isOpen} onOpen={onOpen} onClose={onClose} />\n            </Box>\n        )\n    }\n}"},"lineCount":null}},"error":null,"hash":"148bbc37af584f17ffbb55834e7fde0b","cacheData":{"env":{}}}